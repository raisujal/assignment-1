s=1
for i in range(1,20):
    if i%3==0 and i%5!=0:
        print("Buzz")
    elif i%5==0 and i%3!=0:
        print("fizz")
    elif i%3==0 and i%5==0:
        print("buzz fizz")
    else:
        print(i)
