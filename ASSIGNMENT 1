2.
num = int(input("enter a number: "))
fact = 1
if num < 0:
    print("factorial does not exit for negative numbers")
elif num ==0:
    print("the factorial of 0 is 1")
else:
    for i in range(1,num +1):
        fact = fact*i
print("the factorial of",num,"is",fact)

3.
num = int(input("Enter a number:"))

numberDict = {}
for i in range(1,num+1):
    numberDict[i] = i*i

print(numberDict)

